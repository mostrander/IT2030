
@model IEnumerable<MVCMusicStoreApplication.Models.Album>

@{
   Layout = "~/Views/Shared/_Layout.cshtml";
   ViewBag.Title = "Browse";
}


<head>
    <link href="~/Content/Store.css" rel="stylesheet" />
</head>


<div id="storeDiv" class="container">
    <p>Content Body </p>

    <h2 id="browse-list-header">Here are the list of albums</h2>

    <p>
        <ul>

            @foreach (var item in Model)
            {
                <li>
                    @Html.ActionLink(item.Title, "Details", new { id = item.AlbumId })
                </li>
            }

        </ul>

        @Html.ActionLink("Back to Browse", "Browse")
    </p>
</div>




