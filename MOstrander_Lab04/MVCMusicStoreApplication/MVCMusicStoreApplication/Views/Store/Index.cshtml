
@model IEnumerable<MVCMusicStoreApplication.Models.Album>

@{
   Layout = "~/Views/Store/_StoreLayout.cshtml";
   ViewBag.Title = "Browse";
}


<h1 id="browse-main-header"> ASP.NET MVC MUSIC STORE</h1>


<div>
    <h2>Content Body </h2>

    <h2 id="browse-list-header">Browse by Album</h2>

    <p>
        <ul>

            @foreach (var item in Model)
            {
                <li>
                    @Html.ActionLink(item.Title, "Details", new { id = item.AlbumId })
                </li>
            }

        </ul>


    </p>
</div>




