
@model IEnumerable<MVCMusicStoreApplication.Models.Genre>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Browse";
}

<head>
    <link href="~/Content/Store.css" rel="stylesheet" />
</head>

<body>
    <div id="storeDiv" class="container">
        <p>Content Body </p>

        <h2 id="browse-list-header">Browse by Genre</h2>

        <p>

            <ul>

                @foreach (var item in Model)
                {
                    <li>
                        @Html.ActionLink(item.Name, "Index", new { id = item.GenreId })
                    </li>
                }

            </ul>
        </p>
    </div>

</body>





